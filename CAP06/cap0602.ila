// programa TABELA2
variaveis  
  numerico I, R, SOMA 
  matriz numerico A[5]
inicio
  limpar
  posicionar 1,1
  escrever "Somatorio de elementos impares"
  posicionar 2,1
  para I = 1 ate 5
    posicionar I + 2,1
    escrever "Informe o ", I, "o. valor:"
    posicionar I + 2,23
    ler A[I]
  proximo
  SOMA = 0
  para I = 1 ate 5
    R = resto(A[I], 2)
    se (R <> 0) entao
      SOMA = SOMA + A[I]
    fim_se
  proximo
  escrever " "
  escrever "Soma = ", SOMA
fim
